package section1;

import java.math.BigInteger;

public class QuestionOne {

	public static void main(String[] args) {
		final int n = 100;
		BigInteger res = BigInteger.valueOf(1);
		for(int i=1; i<=n; i++) {
			System.out.println("---> i " + i);
			System.out.println("---> res " + res);

			res = (res*i);
		}
		System.out.println("---> res" + res);
	}

}
